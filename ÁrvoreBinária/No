public class No {

    //arvore Binaria
    
    //atributos do nó
    int key;
    Object o;

    No filhoDireita;
    No filhoEsquerda;


    //opcional
    No pai;

//Construtor
    public No(Object ob, int k){
        this.o = ob;
        this.key = k;
    }
    public No(){}

//Endereco de memoria do nó
    public void setObjKey(Object ob, int k){
        this.o = ob;
        this.key = k;
    }

    public int getKey(){
        return this.key;
    }

//Manipula o nó filhoE
    public No getFilhoEsquerda() {
        return filhoEsquerda;
    }

    public void setFilhoEsquerda(No filhoEsquerda) {
        this.filhoEsquerda = filhoEsquerda;
    }

    public void setKey(int k){
        this.key = k;
    }

//Manipula o nó filhoD
    public No getFilhoDireita() {
        return filhoDireita;
    }

    public void setFilhoDireita(No filhoDireita) {
        this.filhoDireita = filhoDireita;
    }

//Manipula o nó pai
    public No getPai() {
        return pai;
    }

    public void setPai(No pai) {
        this.pai = pai;
    }

//Info guardada no Nó
    public String toString(){
        return key+ " - "+o.toString();
    }
}
